{
    "editor.suggestSelection": "first",
    // Set the default=
    "editor.codeActionsOnSave": [
        "source.fixAll.eslint"
    ],
    "eslint.validate": [
        "typescript",
        "typescriptreact"
    ],
    "eslint.packageManager": "yarn",
    "git.postCommitCommand": "push",
    "diffEditor.ignoreTrimWhitespace": false
}